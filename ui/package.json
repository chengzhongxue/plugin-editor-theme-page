    "@codemirror/language": "^6.11.2",
    "@codemirror/view": "^6.38.1",
    "@formkit/vue": "^1.6.9",
    "@halo-dev/api-client": "^2.21.1",
    "@halo-dev/components": "^2.21.0",
    "@halo-dev/console-shared": "^2.21.0",
    "@he-tree/tree-utils": "0.1.0-alpha.4",
    "@he-tree/vue": "^2.9.4",
    "@imengyu/vue3-context-menu": "^1.5.2",
    "@tanstack/vue-query": "4.37.1",
    "@vueuse/core": "^10.11.1",
    "@vueuse/router": "^10.11.1",
    "axios": "^1.11.0",
    "codemirror": "^6.0.2",
    "dayjs": "^1.11.13",
    "pretty-bytes": "^6.1.1",
    "vue": "^3.5.18",
    "vue-codemirror": "^6.1.1",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@halo-dev/ui-plugin-bundler-kit": "^2.21.2",
    "@iconify/json": "^2.2.373",
    "@rsbuild/core": "^1.4.15",
    "@rsbuild/plugin-vue": "^1.1.1",
    "@rushstack/eslint-patch": "^1.12.0",
    "@tsconfig/node20": "^20.1.6",
    "@types/node": "^20.19.10",
    "@unocss/eslint-config": "^66.4.2",
    "@unocss/webpack": "^66.4.2",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/tsconfig": "^0.5.1",
    "eslint": "^8.57.1",
    "eslint-plugin-vue": "^9.33.0",
    "prettier": "^3.6.2",
    "prettier-plugin-organize-imports": "^4.2.0",
    "typescript": "~5.5.4",
    "unocss": "^66.4.2",
    "unplugin-icons": "^22.2.0",
    "vue-loader": "^17.4.2",
    "vue-tsc": "^2.2.12"
  },
  "packageManager": "pnpm@10.14.0"
